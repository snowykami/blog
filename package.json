{
  "name": "vitepress-blog",
  "version": "1.0.0",
  "private": true,
  "author": "Snowykami",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress dev --host",
    "build": "vitepress build",
    "serve": "vitepress serve --host",
    "watch": "nodemon -e js,json,vue,ts,tsx,css,scss -w ./.vitepress/sugarat/theme -x \"(rm -rf ./.vitepress/cache || echo No Dir) && pnpm install && vitepress dev\""
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@giscus/vue": "^3.0.0",
    "@mdit-vue/shared": "^2.1.3",
    "@octokit/core": "^6.1.2",
    "@sugarat/theme": "file:.vitepress/sugarat/theme",
    "@sugarat/theme-shared": "^0.0.2",
    "@vueuse/core": "^11.1.0",
    "element-plus": "^2.8.4",
    "fast-glob": "^3.3.2",
    "markdown-it": "^14.1.0",
    "module": "^1.2.5",
    "nodemon": "^3.1.7",
    "oh-my-live2d": "^0.19.3",
    "swiper": "^11.1.14",
    "vite": "^5.4.8",
    "vitepress-markdown-timeline": "^1.2.1",
    "vitepress-plugin-pagefind": "^0.4.10",
    "vitepress-plugin-rss": "^0.2.10",
    "vitepress-plugin-tabs": "^0.5.0",
    "vue": "3.4.26"
  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.7.4",
    "pagefind": "^1.1.1",
    "sass": "^1.79.4",
    "typescript": "^5.6.2",
    "vitepress": "1.3.4",
    "vitepress-sidebar": "^1.27.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "type": "module"
}
